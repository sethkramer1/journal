<% @links.each do |link| %>
  <div class="link row clearfix">
    <h1>
      <%= link_to link.title, link %><br>
    </h1>
    <h4>  <%= link.description %><br></h4>
    <p><small class="author">Submitted <%= time_ago_in_words(link.created_at) %> ago by <%= link.user.username %></small>
    </p>



  </div>
<% end %>
